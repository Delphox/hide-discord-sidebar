{
  "name": "Hide Discord Sidebar",
  "version": "2.0",
  "description": "Installs unfolding sidebar for Discord channels and a button that hides/shows server list",
  "manifest_version": 2,
  "permissions": ["*://*.discordapp.com/*"],
  "content_scripts": [{
    "matches": ["*://*.discordapp.com/*"],
    "js": ["hide-discord-sidebar.js"],
    "css": ["hide-discord-sidebar.css"]
  }],
  "icons": {
    "128": "icon128.png"
  }, 
  "browser_action": { "default_icon": "icon128.png" }
}